{
  "name": "primarylendingplatform",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "compile:zksync": "ZKSYNC=true hardhat compile",
    "size-contracts": "hardhat size-contracts",
    "forkmainnet": "hardhat node --fork https://mainnet.infura.io/v3/\"$(cat .env | grep -Eo 'INFURA_KEY=.*' | grep -Eo '\\w+' | tail -1)\"",
    "forkrinkeby": "hardhat node --fork https://rinkeby.infura.io/v3/\"$(cat .env | grep -Eo 'INFURA_KEY=.*' | grep -Eo '\\w+' | tail -1)\"",
    "localDeploy": "hardhat run --network hardhat scripts/deployment/deployToRinkeby.js",
    "localDeployPriceOracle": "hardhat run --network hardhat scripts/deployment/rinkeby/priceOracleDeployment/deployPriceOracle.js",
    "docgen": "npx hardhat docgen",
    "dodoc": "npx hardhat dodoc",
    "deployNewJumpRateModel_ethereum_mainnet": "npx hardhat run --network ethereum_mainnet  ./scripts/NewTransactions/jumpRateModel/deployNewJumpRateModel.js",
    "upgradeBlendingToken_ethereum_mainnet": "hardhat run --network ethereum_mainnet scripts/upgradeProxy/bLendingToken/upgradeBLendingToken.js",
    "deployNewJumpRateModel_ethereum_goerli": "npx hardhat run --network ethereum_goerli ./scripts/NewTransactions/jumpRateModel/deployNewJumpRateModel.js",
    "upgradeBlendingToken_ethereum_goerli": "hardhat run --network ethereum_goerli scripts/upgradeProxy/bLendingToken/upgradeBLendingToken.js",
    "deployPriceOracle_ethereum_goerli": "npx hardhat run --network ethereum_goerli  ./scripts/NewDeployment/priceOracle/deployPriceProviderAggregator.js",
    "deployPrimaryLendingPlatform_ethereum_goerli": "npx hardhat run --network ethereum_goerli  ./scripts/NewDeployment/primaryLendingPlatform/deployPrimaryLendingPlatform.js",
    "deployPLP_ethereum_goerli": "npx hardhat run --network ethereum_goerli  ./scripts/NewDeployment/deployPLP.js",
    "deployPLP_ethereum_mainnet": "npx hardhat run --network ethereum_mainnet  ./scripts/NewDeployment/deployPLP.js",
    "deployPLP_polygon_mumbai": "npx hardhat run --network polygon_mumbai  ./scripts/NewDeployment/deployPLP.js",
    "deployPLP_polygon_mainnet": "npx hardhat run --network polygon_mainnet  ./scripts/NewDeployment/deployPLP.js",
    "deployPLP_arbitrum_goerli": "npx hardhat run --network arbitrum_goerli  ./scripts/NewDeployment/deployPLP.js",
    "deployPLP_arbitrum_mainnet": "npx hardhat run --network arbitrum_mainnet  ./scripts/NewDeployment/deployPLP.js",
    "deployPLP_optimism_goerli": "npx hardhat run --network optimism_goerli  ./scripts/NewDeployment/deployPLP.js",
    "deployPLP_optimism_mainnet": "npx hardhat run --network optimism_mainnet  ./scripts/NewDeployment/deployPLP.js",
    "deployMockToken:zksync": "hardhat deploy-zksync --script deployMockToken.js",
    "deployPriceOracle:zksync": "ZKSYNC=true hardhat deploy-zksync --script deployPriceOracle.js",
    "deployPrimaryLendingPlatform:zksync": "ZKSYNC=true hardhat deploy-zksync --script deployPrimaryLendingPlatform.js",
    "deploy:zksync": "ZKSYNC=true hardhat deploy-zksync",
    "appendUpgradePrimaryIndexToken_goerli": "APPEND_UPGRADE=true npx hardhat run --network ethereum_goerli  ./scripts/upgradeProxy/primaryIndexToken/upgradePrimaryIndexToken.js",
    "upgradePrimaryIndexToken_goerli": "APPEND_UPGRADE=false npx hardhat run --network ethereum_goerli  ./scripts/upgradeProxy/primaryIndexToken/upgradePrimaryIndexToken.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@chainlink/contracts": "^0.2.2",
    "@openzeppelin/contracts": "^4.4.0",
    "@openzeppelin/contracts-upgradeable": "^4.4.0",
    "@primitivefi/hardhat-dodoc": "^0.2.3",
    "hardhat": "^2.12.7",
    "hardhat-contract-sizer": "^2.4.0",
    "js-big-decimal": "^1.3.4",
    "solidity-docgen": "^0.6.0-beta.10"
  },
  "devDependencies": {
    "@ethersproject/hash": "^5.7.0",
    "@ethersproject/web": "^5.7.1",
    "@matterlabs/hardhat-zksync-deploy": "^0.6.1",
    "@matterlabs/hardhat-zksync-solc": "^0.3.14",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@types/node": "^18.13.0",
    "chai": "^4.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.7.2",
    "hardhat-gas-reporter": "^1.0.4",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.16",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "zksync-web3": "^0.13.1"
  }
}
