{
    "overrides": [
      {
        "files": "*.sol",
        "options": {
          "printWidth": 100,
          "tabWidth": 4,
          "useTabs": true,
          "semi": true,
          "singleQuote": false,
          "quoteProps": "as-needed",
          "trailingComma": "es5",
          "bracketSpacing": true,
          "proseWrap": "preserve",
          "endOfLine": "auto"
        }
      }
    ]
  }
  